{
  "name": "concat_s3_q2",
  "title": "concat_s3_q2",
  "summary": "concat_s3_q2",
  "dxapi": "1.0.0",
  "version": "1.0.0",
  "inputSpec": [
    {
      "name": "input1",
      "label": "s3 input",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.fasta.gz","*_s3.fasta.gz"
      ],
      "help": ""
    },
    {
      "name": "input2",
      "label": "q2 input",
      "class": "file",
      "optional": false,
      "patterns": [
        "*_q2.fasta.gz"
      ],
      "help": "The prefix of q2 input will be used as basename"
    },
    {
      "name": "output_extension",
      "class": "string",
      "optional": false,
      "default": "_s4",
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "output",
      "class": "file",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 48
      }
    },
    "interpreter": "bash",
    "release": "16.04",
    "distribution": "Ubuntu",
    "file": "src/code.sh"
  },
  "regionalOptions": {
    "aws:us-east-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_x4"
        }
      }
    }
  }
}
